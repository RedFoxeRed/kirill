@using WebASPnet.Models
@model List<Vegetable> 
@{
    ViewBag.Title = "Овощи таблицей с ценой";
}

<table class="table" style="width:20%">
    <thead>
        <tr>
            <th class="text-center">Наименование</th>
        </tr>
    </thread>
    <tbody>
        @foreach (var vegetable in Model)
            {
                <tr class="text-left">
                    <td>
                        @vegetable.Name 
                        @vegetable.Price
                        </td>
                </tr>
            }
    </tbody>
</table>
